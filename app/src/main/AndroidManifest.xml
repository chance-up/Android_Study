<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.myapplication">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity android:name=".VolleyActivity"></activity>
        <activity android:name=".ScrollActivity" />
        <activity android:name=".LinearInActivity" />
        <activity android:name=".JavaCodeLayout" />
        <activity android:name=".ConstraintLayoutActivity" />
        <activity android:name=".LinearVActivity" />
        <activity android:name=".LinearHActivity" />
        <activity android:name=".LinearLayoutActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>